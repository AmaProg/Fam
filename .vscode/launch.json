{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Reset App",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["reset"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: Create Categories",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": [
        "create",
        "category",
        "-n",
        "Tranport",
        "-d",
        "Depense pour le transport",
        "-c",
        "expense"
      ],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: signup",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["signup"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: add statement",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["add", "statement"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: expense build",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["expense", "build"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: create subcategory",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["create", "subcategory"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: upgrade",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["upgrade"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: Expense build with date",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["expense", "build", "--from", "20240701", "--to", "20240730"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: create transaction",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": [
        "create",
        "transaction",
        "-d",
        "Paiment de loyer",
        "-p",
        "credit card",
        "-a",
        "650.25",
        "--date",
        "20240525",
        "-b",
        "bmo"
      ],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: Invoice build",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["invoice", "build", "--from", "20240101", "--to", "20240731"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: add institution",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["add", "institution"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: create bank-account",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": [
        "create",
        "bank-account",
        "-a",
        "CeliApp",
        "-n",
        "Achat de maison",
        "--amount",
        "23562.36"
      ],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    },
    {
      "name": "Python Debugger: income-statement",
      "type": "debugpy",
      "request": "launch",
      "program": "src/fam/main.py",
      "args": ["finance", "income-statement"],
      "envFile": "${workspaceFolder}/.env-pythonpath"
    }
  ]
}
